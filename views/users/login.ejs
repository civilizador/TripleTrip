<body>

    <% include ../partials/places %>

<div class='wrapperForLogin'>  
  
        <div id='logoPhoto'>
            <img width='100%' src='logoLg.png' >
        </div>
        
        
        <div  id='loginForm'>
          
            <form id='loginFormInside' action="/login" method="POST"  class="ui form">
                <div   class="form-group">
                    <label>User Name</label>
                    <input type="text" name="username" placeholder="Username">
                </div>
                
                <div     class="form-group">
                   <label>Password</label>
                       <input onfocus="this.value=''" type="password" name='password' value="FakePSW" id="myInput"><br><br>
                       <label>Show Password <input type="checkbox" onclick="myFunction()"></label>
                       <br>
                          <script>
                              function myFunction() {
                                  var x = document.getElementById("myInput");
                                  if (x.type === "password") {
                                      x.type = "text";
                                  } else {
                                      x.type = "password";
                                  }
                              }
                          </script>
                </div>
    
                <button class="ui button" type="submit">Submit</button>
                                           <p>Don't have a TripleTrip account? Please <a href='/register'>Register Here</a></p>

            </form>
            
        </div>
  
</div>

